<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_METHODS" value="true" />
  </component>
  <component name="CodeInsightSettings">
    <option name="SHOW_PARAMETER_NAME_HINTS_ON_COMPLETION" value="true" />
    <option name="PARAMETER_INFO_DELAY" value="10" />
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="0" />
  </component>
  <component name="DeclarativeInlayHintsSettings">
    <option name="providerIdToEnabled">
      <map>
        <entry key="js.chain.hints" value="true" />
        <entry key="js.type.hints" value="true" />
        <entry key="ts.chain.hints" value="true" />
        <entry key="ts.enum.hints" value="true" />
        <entry key="ts.type.hints" value="true" />
      </map>
    </option>
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="2" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="JSON.composer.package.version.hints" />
  </component>
  <component name="PhpCodeFoldingSettings">
    <option name="COLLAPSE_METHOD_BODY" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b*@todo\b.*" />
    <pattern pattern="\b*@fixme\b.*" />
    <pattern pattern="#100" />
    <pattern pattern="\b*@deprecated\b.*" />
  </component>
  <component name="XmlSettings">
    <option name="SHOW_XML_ADD_IMPORT_HINTS" value="false" />
  </component>
</application>