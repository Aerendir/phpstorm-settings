      <option name="COMMAND" value="@if [ -f .env.dev.local ]; then export $$(grep -v '^#' .env.dev.local | xargs); fi" />
      <option name="PARAMETERS" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
</toolSet>